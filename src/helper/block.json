{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "icon-grid-unlimited/icon-grid-helper",
    "version": "1.0.0",
    "title": "Icon Grid Helper",
    "category": "widgets",
    "icon": "share-alt",
    "description": "Bridges interactions to an Icon Grid tile via Transition ID",
    "keywords": [
        "grid",
        "helper",
        "link",
        "hover"
    ],
    "supports": {
        "html": false,
        "align": true,
        "spacing": {
            "margin": true,
            "padding": true
        },
        "color": {
            "background": true,
            "gradients": true,
            "text": true
        }
    },
    "attributes": {
        "transitionId": {
            "type": "string",
            "default": ""
        }
    },
    "textdomain": "icon-grid-unlimited",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}